{
	"type": "tag",
	"name": "form",
	"attributes": [
		{
			"type": "attribute",
			"name": "md-layout",
			"css": [
				{
					"property": "width",
					"value": "100%"
				},
				{
					"property": "height",
					"value": "auto"
				},
			    {
			      "property": "padding",
			      "value": "8px 0"
			    }
			]
		}
	],
	"fixes": [
		{
			"after": "[md-layout]> div",
			"css": [
				{
					"property": "max-width",
					"value": "100%"
				},
				{
					"property": "height",
					"value": "auto"
				}
			]
		},
		{
			"after": "[md-layout]> div > *",
			"css": [
				{
					"property": "margin-right",
					"value": "8px"
				},
				{
					"property": "margin-left",
					"value": "8px"
				}
			]
		},
		{
			"after": "[md-layout]> div > md-input:first-child",
			"css": [
				{
					"property": "margin-left",
					"value": "16px"
				}
			]
		},
		{
			"after": "[md-layout]> div > input:last-child",
			"css": [
				{
					"property": "margin-right",
					"value": "16px"
				}
			]
		},
		{
			"after": "[md-layout]> div > *:first-child",
			"css": [
				{
					"property": "margin-left",
					"value": "4px"
				}
			]
		},
		{
			"after": "[md-layout]> div > md-icon:first-child",
			"css": [
				{
					"property": "margin-right",
					"value": "0"
				}
			]
		},
		{
			"after": "[md-layout]> div > *:last-child",
			"css": [
				{
					"property": "margin-right",
					"value": "4px"
				}
			]
		},
		{
			"after": "[md-layout]> div > md-switch:last-child",
			"css": [
				{
					"property": "margin-right",
					"value": "9px"
				}
			]
		},
		{
			"after": "[md-layout]> div> span",
			"css": [
				{
					"property": "margin-right",
					"value": "4px"
				},
				{
					"property": "padding",
					"value": "16px 0"
				}
			]
		},
		{
			"after": "[md-layout]> div> span:first-child",
			"css": [
				{
					"property": "margin-left",
					"value": "16px"
				}
			]
		},
		{
			"after": "[md-layout]> div> span + *",
			"css": [
				{
					"property": "margin-left",
					"value": "4px"
				}
			]
		},
		{
			"after": "[md-layout]> div",
			"css": [
				{
					"property": "display",
					"value": "flex"
				}
			]
		},
		{
			"after": "[md-layout]> div > md-icon + input",
			"css": [
				{
					"property": "margin-left",
					"value": "20px"
				}
			]
		},
		{
			"after": "[md-layout]> div[full]",
			"css": [
				{
					"property": "width",
					"value": "100%"
				}
			]
		},
		{
			"after": "[md-layout]> div> span:not([md-typo])",
			"css": [
				{
					"property": "font-size",
					"value": "16px"
				},
				{
					"property": "font-weight",
					"value": "500"
				},
				{
					"property": "padding",
					"value": "16px 0"
				},
				{
					"property": "line-height",
					"value": "16px"
				}
			]
		}
	]
}