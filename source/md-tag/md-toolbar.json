{
  "type": "tag",
  "name": "md-toolbar",
  "defaults": [
    {
      "name": "md-height",
      "values": [ "x1" ]
    }
  ],
  "css": [
    {
      "property": "display",
      "value": "flex"
    },
    {
      "property": "flex-wrap",
      "value": "wrap"
    },
    {
      "property": "align-content",
      "value": "flex-start"
    },
    {
      "property": "width",
      "value": "100%"
    },
    {
      "property": "top",
      "value": "0"
    },
    {
      "property": "border-bottom",
      "value": "1px solid rgba(0, 0, 0, 0.12)"
    },
    {
      "property": "transition-property",
      "value": "height, box-shadow, background-color, color, top, left"
    },
    {
      "property": "transition-duration",
      "value": "0.25s"
    },
    {
      "property": "transition-timing-function",
      "value": "cubic-bezier(.55,0,.1,1)"
    },
    {
      "property": "z-index",
      "value": "400"
    }
  ],
  "attributes": [
    {
      "type": "attribute",
      "name": "md-position",
      "values": [
        {
          "name": "absolute",
          "css": [
            {
              "property": "position",
              "value": "absolute"
            }
          ]
        },
        {
          "name": "fixed",
          "css": [
            {
              "property": "position",
              "value": "fixed"
            }
          ]
        }
      ]
    }
  ],
  "fixes": [
    {
      "after": ":not([md-position])",
      "css": [
        {
          "property": "position",
          "value": "relative"
        }
      ]
    },
    {
      "after": " md-text",
      "css": [
        {
          "property": "padding-top",
          "value": "12px"
        }
      ],
      "responsive": [
        {
          "target": ["watch","mobile"],
          "css": [
            {
              "property": "padding-left",
              "value": "20px"
            }
          ]
        },
        {
          "target": ["tablet","desktop"],
          "css": [
            {
              "property": "padding-left",
              "value": "24px"
            }
          ]
        }
      ]
    },
    {
      "after": " md-space",
      "css": [
        {
          "property": "display",
          "value": "block"
        },
        {
          "property": "height",
          "value": "48px"
        },
        {
          "property": "flex",
          "value": "1"
        }
      ]
    },
    {
      "after": " > md-row",
      "css": [
        {
          "property": "display",
          "value": "flex"
        },
        {
          "property": "width",
          "value": "100%"
        }
      ],
      "responsive": [
        {
          "target": ["watch", "mobile"],
          "css": [
            {
              "property": "height",
              "value": "56px"
            }
          ]
        },
        {
          "target": ["tablet", "desktop"],
          "css": [
            {
              "property": "height",
              "value": "64px"
            }
          ]
        }
      ]
    },
    {
      "after": " > md-row > *:not(md-tabbar)",
      "responsive": [
        {
          "target": ["watch", "mobile"],
          "css": [
            {
              "property": "margin-top",
              "value": "4px"
            }
          ]
        },
        {
          "target": ["tablet", "desktop"],
          "css": [
            {
              "property": "margin-top",
              "value": "8px"
            }
          ]
        }
      ]
    },
    {
      "after": " > md-row > *:not(md-tabbar):first-child",
      "responsive": [
        {
          "target": ["watch", "mobile"],
          "css": [
            {
              "property": "margin-left",
              "value": "4px"
            }
          ]
        },
        {
          "target": ["tablet", "desktop"],
          "css": [
            {
              "property": "margin-left",
              "value": "8px"
            }
          ]
        }
      ]
    },
    {
      "after": " > md-row > *:not(md-tabbar):last-child",
      "responsive": [
        {
          "target": ["watch", "mobile"],
          "css": [
            {
              "property": "margin-right",
              "value": "4px"
            }
          ]
        },
        {
          "target": ["tablet", "desktop"],
          "css": [
            {
              "property": "margin-right",
              "value": "8px"
            }
          ]
        }
      ]
    },
    {
      "after": " > md-row > md-tabbar",
      "responsive": [
        {
          "target": ["watch", "mobile"],
          "css": [
            {
              "property": "margin-top",
              "value": "8px"
            }
          ]
        },
        {
          "target": ["tablet", "desktop"],
          "css": [
            {
              "property": "margin-top",
              "value": "16px"
            }
          ]
        }
      ]
    }
  ]
}