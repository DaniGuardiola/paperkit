{
  "type": "tag",
  "name": "md-sidemenu",
  "defaults": [
    {
      "name": "md-state",
      "values": ["closed"]
    }
  ],
  "css": [
    {
      "property": "display",
      "value": "block"
    },
    {
      "property": "height",
      "value": "100%"
    },
    {
      "property": "width",
      "value": "80%"
    },
    {
      "property": "position",
      "value": "fixed"
    },
    {
      "property": "top",
      "value": "0"
    },
    {
      "property": "transition",
      "value": "left 0.25s"
    },
    {
      "property": "z-index",
      "value": "500"
    }
  ],
  "attributes": [
    {
      "type": "attribute",
      "name": "md-state",
      "values": [
        {
          "name": "closed",
          "responsive": [
            {
              "target": ["watch","mobile"],
              "css": [
                {
                  "property": "left",
                  "value": "-80%"
                }
              ]
            },
            {
              "target": ["tablet", "desktop"],
              "css": [
                {
                  "property": "left",
                  "value": "-256px"
                }
              ]
            }
          ]
        },
        {
          "name": "open",
          "responsive": [
            {
              "target": ["all"],
              "css": [
                {
                  "property": "left",
                  "value": "0"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "fixes": [
    {
      "after": ">md-list",
      "css": [
        {
          "property": "width",
          "value": "100%"
        }
      ]
    }
  ],
  "responsive": [
    {
      "target": ["tablet", "desktop"],
      "css": [
        {
          "property": "width",
          "value": "256px"
        }
      ]
    }
  ]
}